<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="cellBase" minWidth="-Infinity" prefHeight="75.0" prefWidth="303.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="0.0" prefWidth="303.0" spacing="5.0">
         <children>
            <Label fx:id="nameLbl">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Region prefHeight="27.0" prefWidth="148.0" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#handleViewSnippet" text="View" />
         </children>
      </HBox>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="descriptionLbl" prefHeight="52.0" prefWidth="303.0" wrapText="true" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
